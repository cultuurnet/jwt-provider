{
    "name": "cultuurnet/jwt-provider",
    "description": "Silex application that provides JSON Web Tokens",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "aura/session": "^2.1",
        "cultuurnet/auth": "^3.0",
        "filp/whoops": "^2.5",
        "guzzlehttp/psr7": "^1.3",
        "hassankhan/config": "^2.1",
        "league/container": "^3.3",
        "league/route": "^4.3",
        "monolog/monolog": "~1.11",
        "slim/psr7": "^0.5",
        "symfony/console": "^3.4",
        "symfony/yaml": "^4.3",
        "zendframework/zend-httphandlerrunner": "^1.1",
        "auth0/auth0-php": "^5.7",
        "beberlei/assert": "^3.2",
        "cultuurnet/udb3-api-guard": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "escapestudios/symfony2-coding-standard": "~2.9",
        "squizlabs/php_codesniffer": "~2.5.1",
        "phing/phing": "~2.14",
        "satooshi/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": ["app/", "src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "cultuurnet/*": "source",
            "2dotstwice/*": "source",
            "*": "dist"
        }
    }
}
