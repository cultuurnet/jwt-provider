{
    "name": "cultuurnet/jwt-provider",
    "description": "Silex application that provides JSON Web Tokens",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "aura/session": "^2.1",
        "cultuurnet/auth": "^3.0",
        "cultuurnet/culturefeed-php": "^1.6",
        "cultuurnet/udb3-jwt": "~0.1",
        "cultuurnet/valueobjects": "~3.0",
        "filp/whoops": "^2.15",
        "hassankhan/config": "^2.1",
        "lcobucci/jwt": "~3.1",
        "league/route": "^4.3",
        "monolog/monolog": "~1.11",
        "laminas/laminas-httphandlerrunner": "^2.2",
        "guzzle/guzzle": "^2.6",
        "slim/psr7": "^1.6",
        "league/container": "^4.2",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "~2.9",
        "squizlabs/php_codesniffer": "~2.5.1",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": ["app/", "src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "cultuurnet/*": "source",
            "2dotstwice/*": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse",
        "ci": [
            "composer phpstan",
            "composer test"
        ]
    }
}
