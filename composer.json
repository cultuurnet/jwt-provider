{
    "name": "cultuurnet/jwt-provider",
    "description": "Silex application that provides JSON Web Tokens",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "cultuurnet/auth": "~1.2",
        "silex/silex": "~1.3",
        "deralex/yaml-config-service-provider": "~1.0",
        "knplabs/console-service-provider": "~1.0",
        "monolog/monolog": "~1.11",
        "lcobucci/jwt": "~3.1",
        "nicolopignatelli/valueobjects": "~2.0",
        "cultuurnet/udb3-jwt": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "escapestudios/symfony2-coding-standard": "~2.9",
        "squizlabs/php_codesniffer": "~2.5.1",
        "phing/phing": "~2.14",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": ["app/", "src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
