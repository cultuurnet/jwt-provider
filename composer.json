{
    "name": "cultuurnet/jwt-provider",
    "description": "Silex application that provides JSON Web Tokens",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "cultuurnet/auth": "~1.2",
        "cultuurnet/culturefeed-php": "^1.6",
        "cultuurnet/udb3-jwt": "~0.1",
        "cultuurnet/valueobjects": "~3.0",
        "deralex/yaml-config-service-provider": "~1.0",
        "filp/whoops": "^2.5",
        "guzzlehttp/psr7": "^1.3",
        "hassankhan/config": "^2.1",
        "knplabs/console-service-provider": "~1.0",
        "lcobucci/jwt": "~3.1",
        "league/container": "^3.3",
        "league/route": "^4.3",
        "monolog/monolog": "~1.11",
        "silex/silex": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "escapestudios/symfony2-coding-standard": "~2.9",
        "squizlabs/php_codesniffer": "~2.5.1",
        "phing/phing": "~2.14",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": ["app/", "src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\JwtProvider\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "cultuurnet/*": "source",
            "2dotstwice/*": "source",
            "*": "dist"
        }
    }
}
